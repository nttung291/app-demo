{
  "name": "app-demo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --config app.config.js",
    "start:staging": "EXPO_ENV=staging expo start --config app.config.js",
    "start:prod": "EXPO_ENV=production expo start --config app.config.js",
    "android": "expo run:android",
    "android:dev": "expo run:android --config app.config.js",
    "android:staging": "EXPO_ENV=staging expo run:android --config app.config.js",
    "android:prod": "EXPO_ENV=production expo run:android --config app.config.js",
    "ios": "expo run:ios",
    "ios:dev": "expo run:ios --config app.config.js",
    "ios:staging": "EXPO_ENV=staging expo run:ios --config app.config.js",
    "ios:prod": "EXPO_ENV=production expo run:ios --config app.config.js",
    "web": "expo start --web",
    "web:dev": "expo start --web --config app.config.js",
    "web:staging": "EXPO_ENV=staging expo start --web --config app.config.js",
    "web:prod": "EXPO_ENV=production expo start --web --config app.config.js",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:staging": "eas build --platform android --profile staging",
    "build:android:prod": "eas build --platform android --profile production",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:staging": "eas build --platform ios --profile staging",
    "build:ios:prod": "eas build --platform ios --profile production",
    "test": "jest --watchAll",
    "api": "json-server --watch data/db.json --port 3001"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/drawer": "^7.5.6",
    "@react-navigation/native": "^7.1.6",
    "@tamagui/animations-moti": "^1.132.16",
    "@tamagui/babel-plugin": "^1.132.16",
    "@tamagui/config": "^1.132.16",
    "@tamagui/core": "^1.132.16",
    "@tamagui/font-inter": "^1.132.16",
    "@tamagui/lucide-icons": "^1.132.16",
    "@tamagui/shorthands": "^1.132.16",
    "@tamagui/themes": "^1.132.16",
    "@tanstack/react-query": "^5.85.5",
    "dotenv": "^17.2.1",
    "expo": "~53.0.20",
    "expo-constants": "^17.1.7",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-localization": "^16.1.6",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "i18next": "^25.4.2",
    "i18next-react-native-language-detector": "^1.0.2",
    "msw": "^2.10.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.7.2",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-locale-detector": "^1.0.1",
    "react-native-reanimated": "^4.0.1",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.1",
    "react-native-web": "~0.20.0",
    "react-native-worklets": "^0.4.1",
    "tamagui": "^1.132.16",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-template-literals": "^7.27.1",
    "@types/react": "~19.0.10",
    "concurrently": "^9.2.0",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "json-server": "^1.0.0-beta.3",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
